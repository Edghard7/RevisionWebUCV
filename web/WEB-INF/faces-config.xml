<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <locale-config>
            <default-locale>es</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>Utils.Mensajes</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>bPersona</managed-bean-name>
        <managed-bean-class>Beans.BeanPersona</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bEscuela</managed-bean-name>
        <managed-bean-class>Beans.BeanEscuela</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bTipPersona</managed-bean-name>
        <managed-bean-class>Beans.BeanTipoPersona</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bUsuario</managed-bean-name>
        <managed-bean-class>Beans.BeanUsuario</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bTesis</managed-bean-name>
        <managed-bean-class>Beans.BeanTesis</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bEsquemaT</managed-bean-name>
        <managed-bean-class>Beans.BeanEsquemaTesis</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bLineaInv</managed-bean-name>
        <managed-bean-class>Beans.BeanLineaInvestigacion</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bAsignacion</managed-bean-name>
        <managed-bean-class>Beans.BeanAsignacion</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bRevision</managed-bean-name>
        <managed-bean-class>Beans.BeanRevision</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bDetEsq</managed-bean-name>
        <managed-bean-class>Beans.BeanDetalleEsquema</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bFuncion</managed-bean-name>
        <managed-bean-class>Beans.BeanFuncion</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bChart</managed-bean-name>
        <managed-bean-class>Beans.ChartView</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bObservacion</managed-bean-name>
        <managed-bean-class>Beans.BeanObservacion</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
